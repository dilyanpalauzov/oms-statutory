{
  "name": "oms-statutory",
  "version": "0.22.4",
  "description": "Statutory events module for OMS",
  "main": "lib/server.js",
  "scripts": {
    "lint": "eslint .",
    "db:create": "sequelize db:create",
    "db:setup": "sequelize db:drop; sequelize db:create; sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "test": "NODE_ENV=test npm run db:setup && jest test/api/*.js --runInBand --forceExit",
    "cli": "node --experimental-repl-await -r ./cli.js",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "open-coverage": "open-cli coverage/lcov-report/index.html"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "models/**/*.js",
      "!lib/run.js",
      "!lib/sequelize.js",
      "!lib/logger.js",
      "!lib/fs.js"
    ],
    "testResultsProcessor": "jest-teamcity-reporter",
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [],
  "author": "Sergey Peshkov <github.com/serge1peshcoff>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AEGEE/oms-statutory.git"
  },
  "bugs": {
    "url": "https://github.com/AEGEE/oms-statutory/issues"
  },
  "homepage": "https://github.com/AEGEE/oms-statutory",
  "dependencies": {
    "@bugsnag/js": "^6.3.0",
    "body-parser": "^1.19.0",
    "bugsnag": "^2.4.3",
    "deep-assign": "^3.0.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "express-query-boolean": "^2.0.0",
    "file-type": "^12.3.0",
    "joi": "^14.3.1",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-schedule": "^1.3.2",
    "node-xlsx": "^0.15.0",
    "on-finished": "^2.3.0",
    "pg": "^7.12.1",
    "prom-client": "^11.5.3",
    "read-chunk": "^3.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^3.0.0",
    "sequelize": "^5.18.4",
    "sequelize-cli": "^5.5.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.16.0",
    "faker": "^4.1.0",
    "husky": "^2.3.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "jest-teamcity-reporter": "^0.9.0",
    "nock": "^11.4.0",
    "nyc": "^14.1.1",
    "open-cli": "^5.0.0",
    "timekeeper": "^2.2.0"
  }
}
